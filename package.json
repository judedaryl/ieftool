    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@azure/msal-node": "^1.0.1",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^7.1.0",
    "figlet": "^1.5.0",
    "tsc-watch": "^4.2.9",
    "xml2js": "^0.4.23"
  }
